<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
   <bean id="PSOPublishEditionService" class="com.percussion.pso.workflow.PublishEditionService" >
      <property name="baseUrl" value="http://127.0.0.1" />
      <property name="listenerPort" value="9932" /> 
      <!-- 
      Uncomment these lines to use remote mode. 
      <property name="cmsUser" value="rxpublisher" /> 
      <property name="cmsPassword" value="demo" /> 
       -->
      <property name="workflows" >
          <map> <!-- map of workflows keyed by workflow id -->
             <entry key="5">
                 <map>  <!--  map of transitions keyed by transition id -->
                    <entry key="301">
                        <map> <!-- map of communities keyed by community id  -->
                            <entry key="1001">
                               <value>314</value> <!-- values are edition ids -->
                            </entry>
                            <entry key="1002">
                               <value>315</value>
                            </entry>
                        </map>
                    </entry>
                 </map>
             </entry>
         </map>   
      </property>
   </bean>
</beans>